{
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 80,
    "tabWidth": 4,
    "overrides": [
        {
            "files": "*.json",
            "options": {
                "parser": "json",
                "tabWidth": 4
            }
        },
        {
            "files": "*.css",
            "options": {
                "parser": "css",
                "tabWidth": 4
            }
        },
        {
            "files": [".prettierrc"],
            "options": {
                "parser": "json",
                "tabWidth": 4
            }
        }
    ]
}
